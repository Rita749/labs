#include <stdio.h>
 

unsigned int pr(unsigned int x, unsigned int i)
{
    if (x == 1)
        return -1;
    if (i * i <= x)
    {
        if (x % i == 0)
            return -1; 
        else
            return pr(x, ++i);
    }
    else
        return 0; 
}

 
 
int main()
{
    unsigned int x, y;
    printf("Vvedite chislo: ");
    scanf_s("%d", &x);
    y = pr(x, 2);
    if (y == -1)
        printf("Not simple");
    else
        printf("Simple");
}
